let person = {
    firstName:"Rahul",
    lastName:"Jingar",
    get fullName(){
        return `${person.firstName} ${person.lastName}`
    },
    set fullName(val){
        let parts = val.split(' ');
        this.firstName = parts[0];
        this.lastName = parts[1];
    }
}

try
{
    person.fullName = 1;
}
catch(e)
{
    alert('that is alert in the your full name')
}

console.log(person.fullName);

//Output:- Rahul Jingar

