let person = {
    firstName:"Rahul",
    lastName:"Jingar",
    get fullName(){
        return `${person.firstName} ${person.lastName}`
    },
    set fullName(val){
        let parts = val.split(' ');
        this.firstName = parts[0];
        this.lastName = parts[1];
    }
}


console.log(person.fullName);

//Output:- Rahul Jingar

