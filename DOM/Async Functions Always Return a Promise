async function abcd() {
    return 7;
}
console.log(abcd());

//output:- Promise {[[PromiseState]]: 'fulfilled', [[PromiseResult]]: 7, Symbol(async_id_symbol): 5, Symbol(trigger_async_id_symbol): 1}




async function abcd() {
    return "How are you !!";
}
console.log(abcd());

//output:- Promise {[[PromiseState]]: 'fulfilled', [[PromiseResult]]: 'How are you !!', Symbol(async_id_symbol): 5, Symbol(trigger_async_id_symbol): 1}
